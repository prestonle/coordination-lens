{
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/coordination-analyzer.js",
        "content/content.js"
      ]
    }
  ],
  "author": "Preston Lee Horn",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "content/performance-utils.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "Experimental semantic divergence detector (Research Prototype)",
  "version": "1.1.0",
  "homepage_url": "https://github.com/prestonle/coordination-lens",
  "name": "CoordinationLens",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "storage",
    "downloads"
  ]
}
